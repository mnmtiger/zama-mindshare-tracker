{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/mindshare.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/mindshare",
      "dest": "/api/mindshare.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
